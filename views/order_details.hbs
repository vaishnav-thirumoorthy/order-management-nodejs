{{> customer_header }}
{{> messages }}
<div class="order-details-wrapper">
    <div class="order-details">
        <div class="order-details-card">
            <h3>Order Details</h3>
            <br>
            <hr>
            <h4 style="margin-top: 20px; margin-bottom:20px">Order Number: #{{order_number}}</h4>
            <h4 style="margin-top: 20px; margin-bottom:20px">Date: {{formatDate date "short"}}</h4>
            <br>
            <h4>Items ordered</h4>
            <br>
            {{> order_details_items}}
            <br>
            <p>Click <a href="/orders/{{order_number}}/invoice" target="_blank">here</a> to view the invoice</p>
            <br>
            <br>
            {{#if delivered}}
            {{#unless rating}}
            {{> star_rating}}
            {{/unless}}
            {{#if rating}}
            {{> show_star_rating }}
            {{/if}}
            {{/if}}
        </div>
        
        <div class="order-status">
            <h3>Order Status</h3>
            <br>
            <hr>
            <div class="order-timeline">
                <div class="timeline">
                    <img style="width: 40px; height: 40px" src="data:image/svg+xml;base64," />
                    <p>Order placed</p> 
                </div>
                <div class="timeline">
                    <img style="width: 40px; height: 40px" src="data:image/svg+xml;base64," />
                    <p>Order confirmed</p> 
                </div>
                <div class="timeline">
                    <img style="width: 40px; height: 40px" src="data:image/svg+xml;base64," />                    
                    <p>Order being prepared</p> 
                </div>
                <div class="timeline">
                    <img style="width: 40px; height: 40px" src="data:image/svg+xml;base64," />
                    {{#if delivered}}
                    <p>Out for delivery</p>
                    {{/if}}
                </div>
                <div class="timeline">
                    <img style="width: 40px; height: 40px" src="data:image/svg+xml;base64," />
                    {{#if delivered}}
                    <p>Delivered</p>
                    {{/if}}
                </div>
            </div>
        </div>
    
            
        </div>
    </div>
</div>

